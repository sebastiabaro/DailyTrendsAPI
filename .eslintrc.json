{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/explicit-function-return-type": "off"
    }
}
